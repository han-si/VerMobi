<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:viewmodel="clr-namespace:VerMobi.viewmodel" x:Class="VerMobi.view.Uebersicht" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <Grid>
        <Grid.DataContext>
            <viewmodel:MainViewModel/>
        </Grid.DataContext>
        <Label x:Name="UebersLabel1" Content="Die Datenbank " Margin="10,10,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="Auto"/>
        <Label x:Name="UebersLabelDbName" Content=" " HorizontalAlignment="Left" Margin="107,10,0,0" VerticalAlignment="Top" Width="Auto"/>
        <Label x:Name="UebersLabel2" Content="enthält:" HorizontalAlignment="Left" Margin="125,10,0,0" VerticalAlignment="Top" Width="Auto"/>
        <Grid Margin="0,36,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="87*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Label x:Name="UebersLabelVertr1" Content="Telefon-Verträge:" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="26" Width="Auto"/>
            <Label x:Name="UebersLabelVertrAnz" Content="{Binding AnzVertr}" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,0,0,0" Height="26" Width="Auto"/>
            <Label x:Name="UebersLabelVertr2" Content="davon aktiv:" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="26" Width="Auto"/>
            <Label x:Name="UebersLabelVertrAnzAktiv" Content=" " Grid.Row="0" Grid.Column="3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelTel1" Content="Telefon-Nummern:" HorizontalAlignment="Left" Margin="10,0,0,0" Grid.Row="1" Grid.Column="0" VerticalAlignment="Top" Height="26" Width="Auto"/>
            <Label x:Name="UebersLabelTelAnz" Content=" " Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="1" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelTel2" Content="davon aktiv:" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="1" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelTelAnzAktiv" Content=" " Grid.Column="3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="1" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelSim1" Content="SIM-Karten:" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="0" VerticalAlignment="Top" Height="26" Width="Auto" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelSimAnz" Content=" " Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="2" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelSim2" Content="davon aktiv:" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="2" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelSimAnzAktiv" Content=" " Grid.Column="3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="2" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelGeraete1" Content="Geräte:" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="0" VerticalAlignment="Top" Height="26" Width="Auto" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelGeraeteAnz" Content=" " Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="3" Margin="10,1,0,0"/>
            <Label x:Name="UebersLabelGeraete2" Content="davon in Benutzung:" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="3" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelGeraeteAnzAktiv" Content=" " Grid.Column="3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="3" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelNutzer" Content="Nutzer:" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="0" VerticalAlignment="Top" Height="26" Width="Auto" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelNutzerAnz" Content=" " Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="4" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelFahrz" Content="Fahrzeuge:" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="0" VerticalAlignment="Top" Height="26" Width="Auto" Margin="10,0,0,0"/>
            <Label x:Name="UebersLabelFahrzAnz" Content=" " Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="26" Width="Auto" Grid.Row="5" Margin="10,0,0,0"/>
            <Button x:Name="UebersButtonAktual" Content="aktualisieren" HorizontalAlignment="Left" Margin="8,7,0,0" Grid.Row="6" Grid.Column="0" VerticalAlignment="Top" Width="80"/>
        </Grid>
    </Grid>
</UserControl>
